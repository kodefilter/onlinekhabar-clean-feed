{
	"name" : "Online Khabar Clean Feed",
	"version" : "1.1",
	"description" : "Removing all the advertisements",
	"manifest_version" : 3,
	"permissions": [
		"activeTab"
	 ],
	 "host_permissions": [
		["https://*.onlinekhabar.com/*"]
	 ],
	"content_scripts" : [
		{
			"css": ["style.css"],
			"matches" : ["https://*.onlinekhabar.com/*"]
		
		}
	],
	"icons": { "16": "okclean-16.png",
		"48": "okclean-48.png",
	   "128": "okclean-128.png" }

}